# 生产环境配置
NODE_ENV=production

# API 基础路径 - 生产环境使用实际的后端服务地址
VITE_API_BASE_URL=https://api.example.com/api

# 后端服务地址
VITE_BACKEND_URL=https://api.example.com

# 应用标题
VITE_APP_TITLE=移动端管理后台

# 是否启用 mock 数据
VITE_USE_MOCK=false

# 是否启用调试工具
VITE_USE_DEBUGGER=false

# 是否在控制台显示请求日志
VITE_CONSOLE_REQUEST_LOG=false

# API 请求超时时间（毫秒）
VITE_API_TIMEOUT=10000

# CDN 路径
VITE_CDN_URL=https://cdn.example.com

# 是否开启 gzip 压缩
VITE_BUILD_GZIP=true

# 是否移除 console
VITE_DROP_CONSOLE=true