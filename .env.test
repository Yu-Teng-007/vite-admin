# 测试环境配置
NODE_ENV=production

# API 基础路径
VITE_API_BASE_URL=https://test-api.example.com

# 应用标题
VITE_APP_TITLE=移动端管理后台(测试环境)

# 是否启用 mock 数据
VITE_USE_MOCK=false

# 是否启用调试工具
VITE_USE_DEBUGGER=true

# 是否在控制台显示请求日志
VITE_CONSOLE_REQUEST_LOG=true

# CDN 路径
VITE_CDN_URL=https://test-cdn.example.com

# 是否开启 gzip 压缩
VITE_BUILD_GZIP=false

# 是否移除 console
VITE_DROP_CONSOLE=false 